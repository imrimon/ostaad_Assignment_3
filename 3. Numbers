        <script>
                

                // 3(a)
                function isPrime(num) {
                    for(let i = 2; i < num; i++)
                        if(num % i === 0) return false;
                    return num > 1;
                }

                // 3(b)
                console.log(isPrime(7));
                
        </script>
    </body>
